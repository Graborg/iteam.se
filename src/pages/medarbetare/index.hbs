---
subtitle: Medarbetare
order: 3
layout: default.hbs

navigation: true
contact: true
---

<div class="row partial coworkers">
  <div class="grid-full">
    <ul class="coworkers__list">
      {{#pages route="./src/pages/medarbetare/" start="" type="coworker" size=300}}
        {{#each data}}
          <li>
            <a href="{{ url }}" class="coworkers__content">
              {{#if logo}}
                <img src="{{ logo }}" class="coworkers__image">
              {{/if}}
              <div class="coworkers__name">
                <h4>{{ this.name.first }} <span>{{ this.name.last }}</span></h4>
                <p class="job">{{this.frontmatter.job}}</p>
              </div>
            </a>
          </li>
        {{/each}}
      {{/pages}}
    </ul>
  </div>
</div>
