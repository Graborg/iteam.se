---
subtitle: The Team
order: 3
layout: default.hbs

navigation: false
contact: true

image:
  type: image
  animate: true
  background: fadeIn
  title: The Team
  start: true
  height: 300px
  background-image: url(/content/images/section-top/medarbetare_multiply.jpg)
---
{{>image image}}

<div class="row partial coworkers">
  <div class="grid-full">
    <ul class="coworkers__list">
      {{#pages route="./src/pages/coworkers/" start="" type="coworker" size=300}}
        {{#each data}}

          <li>
            <a href="{{ url }}" class="coworkers__content">
              <div class="coworkers__coworker">
                <template
                  class="coworkers__image"
                  width="320"
                  height="200"
                  image="{{logo}}">
                </template>
                <h4 class="coworkers__coworker-name">{{ this.name.first }} <span>{{ this.name.last }}</span></h4>
                <p class="coworkers__coworker-job">{{this.frontmatter.job}}</p>
              </div>
            </a>
          </li>
        {{/each}}
      {{/pages}}
    </ul>
  </div>
</div>
