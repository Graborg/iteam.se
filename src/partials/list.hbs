<div
  class="
    row
    partial
    list
    {{ type }}
    {{#if background-color}} background-{{{background-color}}} {{/if}}
    {{#if color}} color-{{{color}}} {{/if}}
  "
  {{#if background-color}} style="background: {{{ hexFromString background-color }}}" {{/if}}>
  <div class="grid-full">
    <div class="row flex">
      {{{list items }}}
    </div>
  </div>
</div>
