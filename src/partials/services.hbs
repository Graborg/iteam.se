    narrow
  {{/if}}">
  <div class="grid-full">
    <ul class="services__list">
      {{#pages route="./src/pages/services/" start="" type=""}}
        {{#each data}}
          {{#unless frontmatter.unpublished }}
          <li>
            <h3>
              <a href="/services/{{this.url}}">
                <i class="{{ this.frontmatter.icon }}"></i>
                {{this.title}}
