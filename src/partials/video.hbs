<div
  class="box partial video {{{classList this}}}">

  <video
    {{#if autoplay}} autoplay="autoplay" {{/if}}
    {{#if loop}} loop {{/if}}
    {{#if muted}} muted {{/if}}
    {{#if controls}} controls {{/if}}
    {{#if playbackRate }} playbackRate={{playbackRate}} {{/if}}>
    <source src="{{ file }}" type="video/mp4">
  </video>
</div>
